    // Fetch the user data from the database
    $query = "SELECT * FROM users WHERE id = $userId";
    $result = $db->query($query);
    if ($result->num_rows == 1) {
        $userData = $result->fetch_assoc();
        $user = new User($userData);
        return $user;
    } else {
        die('User not found');
    }
